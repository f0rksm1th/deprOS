{

  "description": "Shutdown the OS from Tor Browser for deprOS",
  "manifest_version": 2,
  "name": "shutdown",
  "version": "1.0",
  "icons": {
    "48": "icons/quit-48.png"
  },

  "applications": {
    "gecko": {
      "id": "kill@depr.os",
      "strict_min_version": "50.0"
    }
  },

  "background": {
    "scripts": ["background.js"]
  },

  "browser_action": {
    "default_icon": "icons/quit-48.png"
  },

  "permissions": ["nativeMessaging"]

}
